<expr> ::= <term> "+" <expr> 
        |  <term> "-" <expr> 
        |  <term> "^" <expr>
        |  <term>

<term> ::= <factor> "*" <term>
        |  <factor> "/" <term>
        |  <factor>

<factor> ::=  "(" <expr> ")"
        |  <const><variable>
        |  <const>

<const> ::= integer

<variable ::= char

